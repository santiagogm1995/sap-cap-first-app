{
    "name": "ZooDeSanti",
    "version": "1.0.0",
    "description": "Generated by cds-services-archetype",
    "license": "ISC",
    "repository": "<Add your repository here>",
    "scripts": {
        "build": "cds build/all --clean",
        "schema": "cds compile srv --to sql > srv/src/main/resources/schema.sql",
        "deploy": "cds deploy",
        "mvn-clean-install": "mvn clean install",
        "start-spring": "mvn spring-boot:run",
        "start": "run-s mvn-clean-install start-spring"
    },
    "dependencies": {
        "@sap/cds": "^3.0.0",
        "sqlite3": "^5.0.0",
        "npm-run-all": "^4.1.5"
    },
    "cds": {
        "build": {
            "target": "."
        },
        "requires": {
            "db": {
                "kind": "sqlite",
                "model": [
                    "db",
                    "srv"
                ],
                "credentials": {
                    "database": "sqlite.db"
                }
            }
        },
        "odata": {
            "version": "v4"
        }
    }
}
